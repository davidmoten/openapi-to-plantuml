@startuml
hide <<Path>> circle
hide <<Response>> circle
hide <<Parameter>> circle
hide empty methods
hide empty fields
skinparam class {
BackgroundColor<<Path>> Wheat
BorderColor<<Path>> Tomato}
set namespaceSeparator none

class "employee" {
}

class "nurse" {
  {field} level : integer
}

class "doctor" {
  {field} income : decimal
}

diamond anon1

"employee" -->"1" "anon1" : "role"

"doctor" --|> "anon1"

"nurse" --|> "anon1"

@enduml