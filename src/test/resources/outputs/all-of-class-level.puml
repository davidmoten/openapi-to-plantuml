@startuml
hide <<Path>> circle
hide <<Response>> circle
hide <<Parameter>> circle
hide empty methods
hide empty fields
set namespaceSeparator none

class "employee" as employee {
}

class "nurse" as nurse {
  {field} level : integer {R}
}

class "doctor" as doctor {
  {field} income : decimal {R}
}

"employee" --> "1" "doctor"

"employee" --> "1" "nurse"

@enduml